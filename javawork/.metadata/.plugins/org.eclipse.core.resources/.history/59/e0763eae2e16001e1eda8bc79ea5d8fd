package day0629;
class Shop{
	
	String sangpum;
	int su;
	
	public Shop(String sang,int su) {
		this.sangpum=sang;
		this.su=su;
	}
	
	public void writeShop()
	{
		System.out.println("상품명: "+sangpum);
		System.out.println("개수: "+su);
	}
}
