package day0629;

class Person{
	
	String name;
	String buseo;
	
	public void getPerson()
	{
		System.out.println("**부모변수출력**");
		System.out.println("이름: "+this.name);
		System.out.println("부서: "+this.buseo);
	}
	
}
///////////////////////
class My extends Person{
	
	int age;
	
	public My(String name,String buseo,int age) {
		this.name=name;
		this.buseo=buseo;
		this.age=age;
	}
}
