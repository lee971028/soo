package day0626;

public class ArrDaCha_06 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int [][]arr;
		arr=new int[2][3]; //2행 3열로 할당(row,column)
		
		//초기값
		arr[0][0]=10;
		arr[0][1]=20;
		arr[0][2]=30;
		
		arr[1][0]=5;
		arr[1][1]=10;
		arr[1][2]=20;
		
		System.out.println("행의갯수:"+arr.length);
		
	}

}
