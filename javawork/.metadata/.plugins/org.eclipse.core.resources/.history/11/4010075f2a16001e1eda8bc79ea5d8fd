package day0629;

public class Manager_04 extends Employee_04{

	String buseo;
	public Manager_04(String name, int sal,String b) {
		super(name, sal); //부모생성자 호출
		buseo=b; //초기화
	}

	@Override
	public String getEmp() {
		// TODO Auto-generated method stub
		return super.getEmp()+","+buseo;
	}
	
	
}
