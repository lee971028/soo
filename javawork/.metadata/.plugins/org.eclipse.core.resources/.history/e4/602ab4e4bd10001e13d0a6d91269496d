package day0622;

public class ExForGugu_04 {

	public static void main(String[] args) {
		// 2~9단까지 구구단
		/*for(int dan=2;dan<=9;dan++)
		{
			for(int j=1;j<=9;j++)
			{
				System.out.println(dan+"X"+j+"="+dan*j);
			}
			
			System.out.println();
		}*/
		
		
		//구구단 으로 break연습
		
		for(int dan=2;dan<=9;dan++)
		{
			for(int j=1;j<=9;j++)
			{
				if(j==5)
					break;
				
				System.out.println(dan+"*"+j+"="+dan*j);
			}
			System.out.println();
		}
			
	}

}
